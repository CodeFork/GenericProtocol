<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netcoreapp2.0;netstandard2.0</TargetFrameworks>
    <PackageId>GenericProtocol</PackageId>
    <Version>1.0.1</Version>
    <Authors>mrousavy, cbarosch</Authors>
    <Company>mrousavy</Company>
    <Product>Generic Protocol</Product>
    <Description>A fast TCP event based buffered server/client protocol for transferring data over the network in .NET Core/Classic</Description>
    <PackageLicenseUrl>https://github.com/mrousavy/GenericProtocol/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/mrousavy/GenericProtocol</PackageProjectUrl>
    <RepositoryUrl>https://github.com/mrousavy/GenericProtocol</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <PackageTags>tcp socket protocol client server</PackageTags>
    <AssemblyVersion>1.0.0.1</AssemblyVersion>
    <FileVersion>1.0.0.1</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="ZeroFormatter" Version="1.6.4" />
  </ItemGroup>

</Project>
